<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.yinrun.dao.SysRolePowerDao">
	<delete id="deleteByRoleIdAndPowerId">
		DELETE 
		FROM
			sys_role_power
		WHERE
			role_id = #{roleId} and power_id = #{powerId}
	</delete>
</mapper>